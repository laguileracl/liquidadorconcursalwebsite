{
  "version": 2,
  "name": "liquidadorconcursalwebsite",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "redirects": [
    { "source": "/index-astroship", "destination": "/", "permanent": true },
    { "source": "/index-astroship/(.*)", "destination": "/", "permanent": true },
    { "source": "/index-flowbite-backup", "destination": "/", "permanent": true },
    { "source": "/index-flowbite-backup/(.*)", "destination": "/", "permanent": true }
  ]
}
