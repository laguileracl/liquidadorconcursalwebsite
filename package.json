    "@iconify-json/tabler": "^1.2.22",
    "@tailwindcss/vite": "^4.1.13",
    "astro": "^5.13.7",
    "astro-icon": "^1.1.5",
    "astro-navbar": "^2.4.0",
    "astro-seo": "^0.8.4",
    "flowbite": "^3.1.2",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.3",
